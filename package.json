{
  "name": "electron-security-scanner",
  "version": "1.0.0",
  "description": "Desktop security scanner using CDP + ZAP + Nuclei architecture",
  "main": "./src/main/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "electron .",
    "build:prod": "npm run build && electron-forge make",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write ."
  },
  "keywords": [
    "security",
    "scanner",
    "electron",
    "zap",
    "nuclei"
  ],
  "author": "Security Team",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^4.0.9",
    "electron": "^35.0.0",
    "handlebars": "^4.7.8",
    "keytar": "^7.9.0",
    "lucide-react": "^0.563.0",
    "pdfkit": "^0.17.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@testing-library/react": "^16.3.2",
    "@types/keytar": "^4.4.0",
    "@types/node": "^22.0.0",
    "@types/pdfkit": "^0.17.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.0.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
